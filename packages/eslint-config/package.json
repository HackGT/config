{
  "name": "@hex-labs/eslint-config",
  "version": "1.3.1",
  "description": "HexLabs shareable ESLint config for TypeScript projects.",
  "private": false,
  "homepage": "https://github.com/hackgt/config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackgt/config",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/hackgt/config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "test": "tape __tests__/*.js",
    "output": "eslint --config=.eslintrc.js --print-config output.json > output.json"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "typescript": ">=5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "tape": "^5.6.6",
    "typescript": "^5.1.6"
  }
}
