{
  "name": "@hex-labs/eslint-config-react",
  "version": "1.3.1",
  "description": "HexLabs shareable ESLint config for TypeScript React projects.",
  "private": false,
  "homepage": "https://github.com/hackgt/config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackgt/config",
    "directory": "packages/eslint-config-react"
  },
  "bugs": {
    "url": "https://github.com/hackgt/config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "test": "tape __tests__/*.js",
    "output": "eslint --config=.eslintrc.js --print-config output.json > output.json"
  },
  "peerDependencies": {
    "eslint": ">= 8",
    "react": ">= 17",
    "typescript": ">= 4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "tape": "^5.6.1",
    "typescript": "^4.9.3"
  }
}
