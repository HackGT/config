{
  "name": "@hex-labs/eslint-config-react",
  "version": "1.1.12",
  "description": "HexLabs shareable ESLint config for TypeScript React projects.",
  "private": false,
  "homepage": "https://github.com/hackgt/config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackgt/config",
    "directory": "packages/eslint-config-react"
  },
  "bugs": {
    "url": "https://github.com/hackgt/config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "test": "tape __tests__/*.js",
    "output": "eslint --config=.eslintrc.js --print-config output.json > output.json"
  },
  "peerDependencies": {
    "eslint": ">= 7",
    "react": ">= 17",
    "typescript": ">= 4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "tape": "^5.2.2",
    "typescript": "^4.1.3"
  }
}
