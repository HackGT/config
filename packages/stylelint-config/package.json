{
  "name": "@hex-labs/stylelint-config",
  "version": "1.2.0",
  "description": "HexLabs shareable Stylelint config",
  "private": false,
  "homepage": "https://github.com/hackgt/config",
  "license": "MIT",
  "repository": {
